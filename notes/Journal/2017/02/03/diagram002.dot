digraph models {
  rankdir=TB;
  node [shape=box, style="filled,rounded", fillcolor=lightskyblue2];

  {rank=same; Element; Commitment; Event;}
  {rank=same; Membership, Request;}
  Request -> Event;
  Request -> "Group Element";
  Event -> Commitment [dir="back"];
  Commitment -> Element;
  Commitment -> Request;

  Membership -> "Group Element";
  Membership -> Element;
}