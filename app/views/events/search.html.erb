<h1>Events</h1>

<%= will_paginate @found_events %>
<table class="zftable">
  <thead>
    <tr>
      <th>When</th>
      <th>Event</th>
<% if @full_details %>
      <th>Properties</th>
<% end %>
      <th>Where</th>
<% if @full_details %>
      <th>Organiser</th>
      <th>Owner</th>
<% end %>
    </tr>
