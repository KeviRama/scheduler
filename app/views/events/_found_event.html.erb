<tr>
  <td>
    <%=
      link_to found_event.starts_at_text,
              schedule_show_path + "?date=#{found_event.starts_at.to_date.strftime("%Y-%m-%d")}"
    %>
  </td>
  <td title="<%= found_event.body %>">
    <%= truncate(found_event.body, :length => 40) %>
  </td>
  <td title="<%= found_event.properties.collect {|p| p.name}.join(",")%>">
    <%= found_event.properties.collect {|p|
          truncate(p.name, :length => 14)
        }.join("<br/>").html_safe %>
  </td>
  <td><%= found_event.locations.collect {|l| l.name}.join("<br/>").html_safe %></td>
  <td><%= found_event.owners_initials %></td>
</tr>
