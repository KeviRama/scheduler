<tr>
  <td style="background-color:<%= interest.colour %>;color:white" class="dynamic-element" element_id="<%= interest.element_id %>" title="<%= interest.element.id %>">
    <%= interest.element.name %>
  </td>
  <td>
    <input class="clicky" type="checkbox" checked  element_id="<%= interest.element_id %>" />
  </td>
  <td>
    <%= link_to '&#215;'.html_safe, interest, remote: true, method: :delete %>
  </td>
</tr>

