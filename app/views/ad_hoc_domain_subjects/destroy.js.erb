<% @erstwhile_staff.each do |es| %>
    window.ahdUpdate([
      {
        action: 'update_staff_subjects',
        staff_id: <%= es.id %>,
        subject_listing: "<%=j(
          render object: es, partial: 'ad_hoc_domain_staffs/staff_inner') %>"
      }
    ]);
<% end %>
window.ahdUpdate([
  {
    action: 'delete_subject',
    subject_id: <%= @ad_hoc_domain_subject.id %>
  },
  {
    action: 'clear_errors'
  }
]);
