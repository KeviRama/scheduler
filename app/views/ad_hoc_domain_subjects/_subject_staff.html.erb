<div id='ahd-subject-staff-<%= subject_staff.id %>' class='foldable<%=
@folded ? " folded" : "" %>'>
  <% if subject_staff.ad_hoc_domain_staffs.size == 0 %>
    <%= ahd_row(
      contents: {
        no_staff: "None defined yet."
      }
    ) %>
  <% end %>
  <%=
    render(
      partial: 'ad_hoc_domain_staffs/ad_hoc_domain_staff',
      collection: subject_staff.ad_hoc_domain_staffs.sort,
      locals: {
        ad_hoc_domain_subject: subject_staff
      }
    )
  %>
  <%=
    ahd_new_staff_form(subject_staff.ad_hoc_domain_cycle,
                       subject_staff)
  %>
</div>

