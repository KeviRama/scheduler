<%= form_for(@user, html: {method: :get, onsubmit: "return window.linkToUser();" } ) do |f| %>
  <%= f.autocomplete_field :name,
                           autocomplete_user_name_users_path,
                           :id_element => '#user_id',
                           :placeholder => "Find user",
                           "data-auto-focus" => true %>
  <%= f.text_field :id, :type => :hidden %>
<% end %>
