<table style="current_user_table">
<tr><td style="width:80%;"> </td><td style="width:10px;"></td><td style="width:10px;"></td></tr>
<tr><th style="width:80%">Me</th><th></th></tr>
<%= render user.ownerships.me %>
<% if user.create_events? %>
<tr>
  <td style="background-color:<%= current_user.colour_not_involved %>;color:white">
    My events
  </td>
  <td>
    <input type="checkbox" value="on" />
  </td>
  <td></td>
</tr>
<% end %>
<% if user.ownerships.notme.size > 0 %>
<tr><th>My assets</th><th></th></tr>
  <%= render user.ownerships.notme %>
<% end %>
<tr><th>Watching</th><th></th></tr>
<%= render user.interests %>
<tr><th>Quick add</th><th></th></tr>
<tr>
  <td colspan="3">
    <%= form_for(@interest, remote: true) do |f| %>
      <%= f.autocomplete_field :name,
                               autocomplete_element_name_elements_path,
                               :id_element => '#interest_element_id' %>
      <%= f.text_field :element_id, :type => :hidden %>
      <%= f.submit "Find" %>
    <% end %>
  <td>
</tr>
</table>
