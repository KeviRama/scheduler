<h5>Me</h5>
<ul>
  <% user.ownerships.me.each do |o| %>
    <li><span style="background-color:<%= o.colour %>;color:white">
      <%= o.element.name %>
    </span></li>
  <% end %>
</ul>
<% if user.ownerships.notme.size > 0 %>
  <h5>My assets</h5>
  <ul>
    <% user.ownerships.notme.each do |o| %>
      <li><span style="background-color:<%= o.colour %>;color:white">
        <%= o.element.name %>
      </span></li>
    <% end %>
  </ul>
<% end %>
<h5>Watching</h5>
<ul>
  <%= render user.interests %>
</ul>
<h5>Quick add</h5>
<%= form_for(@interest, remote: true) do |f| %>
  <%= f.autocomplete_field :name,
                           autocomplete_element_name_elements_path,
                           :id_element => '#interest_element_id' %>
  <%= f.text_field :element_id, :type => :hidden %>
  <%= f.submit "Find" %>
<% end %>
