<div id='ahd-subject-listing'>
  <% if @ad_hoc_domain_cycle.ad_hoc_domain_subjects.size == 0 %>
    <%= ahd_row(
      contents: { no_subject: "None defined yet." },
      row_classes: %w(placeholder)) %>
  <% else %>
    <%=
      render(
        partial: 'ad_hoc_domain_subjects/subject_outer',
        collection: @ad_hoc_domain_cycle.ad_hoc_domain_subjects.sort)
      %>
  <% end %>
  <%=
    ahd_row(
      contents: {
        subject_form: ahd_new_subject_form(@ad_hoc_domain_cycle)
      },
      row_classes: %w(underrule overrule)
    )
    %>
</div>

