
<h1>Arranging <%= @ad_hoc_domain.name %></h1>

<section class='ahd-listing'>
  <ol>
    <%=
      ahd_row(
        row_classes: %w(heading),
        classes1: %w(heading),
        text1: "Subject"
      )
      %>
    <% if @ad_hoc_domain.ad_hoc_domain_subjects.empty? %>
      None defined yet.
    <% else %>
      <%= render @ad_hoc_domain.ad_hoc_domain_subjects.sort %>
    <% end %>
  </ol>
</section>

